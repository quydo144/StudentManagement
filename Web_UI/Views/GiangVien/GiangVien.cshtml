@model EntityFrameWork_BaiTapLon.Entities_BaiTapLon.GiangVien

<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    <link rel="shortcut icon" href="https://media.iuh.edu.vn/Media/2_sviuh/Images/icon65d669c1-0-e.ico">
    <link rel="stylesheet" type="text/css" href="~/Content/main.css">
    <link href="~/Content/components.css" rel="stylesheet" />
    <link href="~/Content/profile.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="/Content/style.css?v=15">
    <link rel="stylesheet" type="text/css" href="/Content/base.css">
    <link rel="stylesheet" type="text/css" href="~/Content/responsive.css">
    <link href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" crossorigin="anonymous" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/js/all.min.js" crossorigin="anonymous"></script>
    <style>
        .tt {
            font-style: inherit;
            color: black;
            width: inherit;
            height: inherit;
        }

        .profile-userpic {
            height: auto;
        }
    </style>
</head>
<body>


    <div id="focus-overlay"></div>
    <div class="wrapper">

        <header class="header">
            <div class="container">
                <div class="header-content">

                    <div class="menu-btn">
                        <a href="#" title=""><i class="fa fa-bars"></i></a>
                    </div>
                    <div class="user-account dropdown">
                        <div class="user-info" data-toggle="dropdown">
                            <a href="#" title="">@Model.TenGiangVien</a>
                            <i class="fa fa-caret-down" aria-hidden="true"></i>
                        </div>
                        <div class="user-account-info dropdown-menu pull-right">
                            <ul class="us-links">
                                <li><a href="/SinhVien/Logout" title="">Đăng xuất</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="menu-top">
                        <ul>
                            <li>
                                <a href="/dashboard.html" title="">
                                    <span><i class="fa fa-home" aria-hidden="true"></i></span>
                                    Trang chủ
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </header>
        <div class="main-content main-dashboard">
            <div class="container">
                <div class="main-section-content" id="contnet">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="box-df profile-ds-info">
                                <div class="portlet">

                                    <div class="portlet-title">
                                        <div class="caption">
                                            <span class="caption-subject bold">Thông tin giảng viên</span>
                                        </div>
                                    </div>
                                    <div class="portlet-body">
                                        <div class="row">
                                            <div class="col-sm-3">
                                                <div class="profile-userpic">
                                                    <img src="data:image/png;base64," class="img-responsive" style="object-fit: cover;" height="200" width="200" />
                                                </div>
                                            </div>
                                            <div class="col-sm-9">
                                                <form class="form-horizontal">
                                                    <div class="form-body">
                                                        <div class="form-group">
                                                            <label class="col-xs-6">Họ Tên : <span class="bold">@Model.TenGiangVien</span></label>
                                                            <label class="col-xs-6">Mã giảng viên :  <span class="bold">@Model.GiangVienid</span></label>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-xs-6">Giới tính : <span class="bold">@Model.Gioitinh</span></label>
                                                            <label class="col-xs-6">Địa chỉ : <span class="bold">@Model.Diachi</span></label>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-xs-6">Email : <span class="bold">@Model.Email</span></label>
                                                            <label class="col-xs-6">Chuyên ngành : <span class="bold">@Model.ChuyenNganh</span></label>
                                                        </div>
                                                        <div class="form-group">
                                                            <label class="col-xs-6">Ngày sinh : <span class="bold">@Model.Ngaysinh</span></label>

                                                        </div>

                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="featured">

                        <div class="featured-item">
                            <a href=@Url.Action("DanhSachMonHoc","GiangVien") title="Danh sách môm học">
                                <div class="box-df">
                                    <div class="icon">
                                        <img src="data:image/png;base64," alt="">
                                    </div>
                                    Danh sách môm học
                                </div>
                            </a>
                        </div>
                        <div class="featured-item">
                            <a target=_blank href="@Url.Action("DanhSachLopHocPhan","GiangVien")" title="Danh sách lớp học phần">
                                <div class="box-df">
                                    <div class="icon">
                                        <img src="data:image/png;base64," alt="">
                                    </div>
                                    Danh sách lớp học phần
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>



